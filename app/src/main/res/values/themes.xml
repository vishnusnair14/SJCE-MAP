<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Theme.SJCEMAP" parent="Theme.Material3.Light">

<!--        Primary brand color.-->
        <item name="colorPrimary">#D3560D</item>
        <item name="colorPrimaryVariant">#B44A0E</item>
        <item name="colorOnPrimary">@color/white</item>


        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">#021E35</item>

        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/anonymous_pro_bold</item>
        <item name="android:forceDarkAllowed">false</item>
    </style>

    <style name="Theme.SJCEMAP.NoActionBar" parent="Theme.Material3.Light">

        <!-- Primary brand color. -->
        <item name="colorPrimary">#D3560D</item>
        <item name="colorPrimaryVariant">#B44A0E</item>
        <item name="colorOnPrimary">@color/white</item>


        <!-- Status bar color. -->
        <item name="android:statusBarColor">#021E35</item>

        <!-- Customize your theme here. -->
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:forceDarkAllowed">false</item>
        <item name="android:fontFamily">@font/anonymous_pro_bold</item>
    </style>

    <style name="Theme.SJCEMAP.AppBarOverlay" parent="ThemeOverlay.Material3.Light" />
    <!---->
    <style name="Theme.SJCEMAP.PopupOverlay" parent="ThemeOverlay.Material3.Light" />
</resources>